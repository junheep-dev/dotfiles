{
    "global": { "show_in_menu_bar": false },
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Map F4 to Command+Space",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "f4",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "spacebar",
                                        "modifiers": ["command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Korean <-> English by tapping left ctrl",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_control",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "left_control",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [{ "key_code": "f18" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "When Esc is pressed for escaping from vim editor, the language will be changed to English",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "com.microsoft.VSCode",
                                            "com.apple.Terminal",
                                            "com.googlecode.iterm2",
                                            "com.github.wez.wezterm",
                                            "com.culturedcode.ThingsMac",
                                            "com.mitchellh.ghostty"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": { "key_code": "escape" },
                                "to": [
                                    { "select_input_source": { "language": "en" } },
                                    { "key_code": "escape" }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "com.mitchellh.ghostty",
                                            "com.github.wez.wezterm",
                                            "com.googlecode.iterm2"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "q",
                                    "modifiers": { "mandatory": ["left_control"] }
                                },
                                "to": [
                                    { "select_input_source": { "language": "en" } },
                                    {
                                        "key_code": "q",
                                        "modifiers": ["left_control"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": " Left command tap to ctrl-alt-b(tmux prefix)",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_command",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "left_command",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "b",
                                        "modifiers": ["left_control", "left_alt"]
                                    },
                                    { "select_input_source": { "language": "en" } }
                                ],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
